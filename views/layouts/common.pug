html
    head
        link(rel="stylesheet",href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css")
        link(rel="stylesheet", href="/styles/custom.css")  
        
        script(src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous")
        script(src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous")
        script(src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous")
    body(style='margin: 10px 40px')
        nav.navbar.navbar-expand-lg.navbar-light.bg-light
            a(href="/", class='navbar-brand')
                img(src="/images/logo-book.jpg", alt="logo",width="35",height="35", class="d-inline-block align-top border mr-2")
                span(class='navbar-brand mb-0 h1') M_Users
                button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
                    span.navbar-toggler-icon
            #navbarSupportedContent.collapse.navbar-collapse
                ul.navbar-nav.mr-auto
                    li.nav-item
                        a.nav-link(href='/',class='navbar-brand mb-0 h1') Home 
                        span.sr-only (current)
                    li.nav-item
                        a.nav-link(href='/users', class='navbar-brand mb-0 h1') List Users
                    li.nav-item.dropdown
                        a#navbarDropdownMenuLink.nav-link.dropdown-toggle(href='#', role='button', data-toggle='dropdown', aria=(haspopup='true',expanded='false'), class='navbar-brand mb-0 h1') Feature
                        .dropdown-menu(aria-labelledby='navbarDropdownMenuLink')
                            a.dropdown-item(href='#', class='navbar-brand mb-0 h1') Action
                            a.dropdown-item(href='#', class='navbar-brand mb-0 h1') Another action
                            a.dropdown-item(href='#', class='navbar-brand mb-0 h1') Something else here
                    li.nav-item
                        a.nav-link(href='/about', class='navbar-brand mb-0 h1') About
                form.form-inline.my-2.my-lg-0
                    input.form-control.mr-sm-2(type='search', placeholder='Search', aria-label='Search')
                button.btn.btn-outline-success.my-2.my-sm-0(type='submit') Search
            
        .container.pt-5
            block content